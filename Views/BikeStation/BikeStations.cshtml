@{
    var stations = ViewBag.allstations;

}



<table class="table">
    <thead>
        <tr>
            <th scope="col">Nom</th>
            <th scope="col">Nombre de velo disponible</th>
            <th scope="col">Nombre d'emplacement disponible</th>
            <th scope="col">Adresse</th>
            <th scope="col">Handle</th>
        </tr>
    </thead>
    <tbody>
        


        @foreach (var station in stations)
        {
            <tr>
                <td>@station.name</td>
                <td>@station.available_bikes</td>
                <td>@station.bike_stands</td>
                <td>@station.address</td>
                <td></td>
            </tr>
            
        }
    </tbody>
</table>